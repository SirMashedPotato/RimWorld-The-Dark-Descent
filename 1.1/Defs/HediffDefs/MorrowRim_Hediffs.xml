<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffDef>
		<defName>MorrowRim_Discharge</defName>
		<label>electrical discharge</label>
		<description>Prevents another discharge until recovered.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<defaultLabelColor>(0.8, 0.6, 0.4)</defaultLabelColor>
		<maxSeverity>1</maxSeverity>
		<stages>
			<li>
				<label>recovering</label>
				<becomeVisible>false</becomeVisible>
			</li>
		</stages>
		<comps>
		  <li Class="HediffCompProperties_SelfHeal">
			<healAmount>0.08</healAmount>
		  </li>
		</comps>
	</HediffDef>
	  
</Defs>